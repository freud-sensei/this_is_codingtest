ys\n","# input = lambda: sys.stdin.readline().rstrip()\n","\n","N, M = map(int, input().split())\n","grid = []\n","\n","for _ in range(N):\n","    grid.append(list(map(int, input().split())))\n","\n","tetro = [\n","    # 일자\n","    [(0, 1), (0, 2), (0, 3)],\n","    [(1, 0), (2, 0), (3, 0)],\n","\n","    # 정사각형\n","    [(0, 1), (1, 0), (1, 1)],\n","\n","    # L자\n","    [(1, 0), (2, 0), (2, 1)],\n","    [(0, 1), (0, 2), (1, 0)],\n","    [(0, 1), (1, 1), (2, 1)],\n","    [(0, 1), (0, 2), (-1, 2)],\n","\n","    # 뒤집은 L자\n","    [(1, 0), (2, 0), (2, -1)],\n","    [(1, 0), (1, 1), (1, 2)],\n","    [(0, 1), (1, 0), (2, 0)],\n","    [(0, 1), (0, 2), (1, 2)],\n","\n","    # ㄹ자\n","    [(1, 0), (1, 1), (2, 1)],\n","    [(0, 1), (1, 0), (1, -1)],\n","\n","    # 뒤집은 ㄹ자\n","    [(1, 0), (1, -1), (2, -1)],\n","    [(0, 1), (1, 1), (1, 2)],\n","\n","    # ㅗ자\n","    [(0, 1), (0, 2), (1, 1)],\n","    [(1, 0), (1, 1), (2, 0)],\n","    [(1, -1), (1, 0), (1, 1)],\n","    [(-1, 1), (0, 1), (1, 1)]\n","]\n","\n","def check(x, y):\n","    max_result = 0\n","    for tet in tetro:\n","        result = 0\n","        for dx, dy in [(0, 0)] + tet:\n","            nx, ny = x + dx, y + dy\n","            if 0 <= nx < N and 0 <= ny < M:\n","                result += grid[nx][ny]\n","            else:\n","                break\n","        max_result = max(result, max_result)\n","    return max_result\n","\n","answer = 0\n","\n","for x in range(N):\n","    for y in range(M):\n","        answer = max(answer, check(x, y))\n","print(answer)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"tvTml5Pv4WUf","executionInfo":{"status":"ok","timestamp":1738504232925,"user_tz":-540,"elapsed":15202,"user":{"displayName":"­송상록 / 학생 / 심리학과","userId":"18164983390300526999"}},"outputId":"e4e82bba-2f06-490a-cf94-adf31b5d474e"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["4 10\n","1 2 1 2 1 2 1 2 1 2\n","2 1 2 1 2 1 2 1 2 1\n","1 2 1 2 1 2 1 2 1 2\n","2 1 2 1 